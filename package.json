{
  "name": "UFSIT-Portal",
  "version": "1.0.0",
  "description": "A UFSIT specific member portal",
  "main": "app.js",
  "scripts": {
    "start": "npm run build && node app.js",
    "dev": "concurrently --kill-others \"npm run node-monitor\" \"npm run angular-monitor\"",
    "node-monitor": "nodemon app.js --ignore angular/*",
    "angular-monitor": "ng serve --proxy proxy.conf.json",
    "mon": "type -P \"nodemon\" && nodemon app.js || echo \"Nodemon not installed.\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint",
    "build": "ng build --prod"
  },
  "author": "Elan Rasabi",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.9",
    "angular-timeago": "^0.4.6",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "express-sanitize-escape": "^1.1.0",
    "fs": "0.0.2",
    "http": "0.0.0",
    "http-errors": "^1.6.2",
    "json-sql": "^0.3.11",
    "lodash": "^4.13.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "passport": "^0.3.2",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "engines": {
    "node": ">= 7.6.0"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "eslint": "^4.16.0",
    "eslint-config-google": "^0.9.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
